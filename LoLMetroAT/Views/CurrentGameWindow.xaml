<Controls:MetroWindow x:Class="LoLMetroApp.Windows.CurrentGameWindow"
                      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                      xmlns:Controls="http://metro.mahapps.com/winfx/xaml/controls"
                      xmlns:ValueConverter="clr-namespace:LoLMetroAT.ValueConverter"
                      Height="520" Width="1160"
                      Foreground="Goldenrod"
                      BorderBrush="{DynamicResource AccentColorBrush}"
                      NonActiveBorderBrush="{DynamicResource AccentColorBrush}"
                      NonActiveGlowBrush="{DynamicResource AccentColorBrush}"
                      NonActiveWindowTitleBrush="{DynamicResource AccentColorBrush}"
                      OverrideDefaultWindowCommandsBrush="{DynamicResource AccentColorBrush}"
                      BorderThickness="2"
                      ShowIconOnTitleBar="False"
                      ResizeMode="NoResize" 
                      WindowStartupLocation="CenterScreen" 
                      Topmost="True" 
                      ShowSystemMenuOnRightClick="False" 
                      ShowMinButton="False" 
                      ShowMaxRestoreButton="False" 
                      ShowCloseButton="False" 
                      FocusManager.IsFocusScope="False"
                      RightWindowCommandsOverlayBehavior="Never" 
                      ShowActivated="False" 
                      TitleCaps="False" 
                      ShowTitleBar="False" 
                      IsEnabled="False" 
                      ShowInTaskbar="False" 
                      WindowStyle="None"
                      >

    <Window.Resources>
        <ResourceDictionary>
            <ValueConverter:LoLStaticDataConverter x:Key="LoLStaticDataConverter"/>
        </ResourceDictionary>
    </Window.Resources>
    <Controls:MetroWindow.Background>
        <ImageBrush ImageSource="/LoLMetroAT;component/Images/CurrentGameBackImg.jpg" Opacity="0.4" Stretch="UniformToFill"/>
    </Controls:MetroWindow.Background>

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="60" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>

        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="80"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <StackPanel 
