                        CornerRadius="2"
                        Grid.Row="1">

                        <Grid>

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>

                            <Border 
                                ToolTip="{Binding BlueGameItemDetailPropertys[0].Participant.Stats.ChampLevel}"
                                Grid.Column="0"
                                HorizontalAlignment="Left"
                                CornerRadius="6" Width="36" Height="36"
                                
                                >

                                <Border.Background>
                                    <ImageBrush Stretch="UniformToFill" ImageSource="{Binding BlueCurrentGameItemDetailPropertys[0].ChampionId, Converter={StaticResource LoLStaticDataConverter}, ConverterParameter={x:Static ValueConverter:ConverterType.ChampionImg}}"/>
                                </Border.Background>
                            </Border>

                            <StackPanel Grid.Column="1" Width="400" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="50,0,0,0">

                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="Auto" />
                                    </Grid.ColumnDefinitions>

                                    <StackPanel Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center">
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="Auto" />
                                            </Grid.ColumnDefinitions>

                                            <StackPanel Grid.Column="0" Orientation="Vertical" HorizontalAlignment="Left">
                                                <TextBlock Width="50" TextAlignment="Left" Text="{Binding BlueCurrentGameItemDetailPropertys[0].TotalSessionsWon, StringFormat='勝 {0}', UpdateSourceTrigger=PropertyChanged}"/>
                                                <TextBlock Width="50" TextAlignment="Left" Text="{Binding BlueCurrentGameItemDetailPropertys[0].TotalSessionsLost, StringFormat='敗 {0}', UpdateSourceTrigger=PropertyChanged}"/>
                                            </StackPanel>

                                            <StackPanel Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="10 0 0 0">
