                                <Border 
                                    CornerRadius="0" Width="23" Height="23" Grid.Column="0"
                                    
                                    >

                                    <Border.Background>
                                        <ImageBrush Stretch="UniformToFill" ImageSource="{Binding BlueCurrentGameItemDetailPropertys[0].ProfileIcon, Converter={StaticResource LoLStaticDataConverter}, ConverterParameter={x:Static ValueConverter:ConverterType.ProFileIcon}}"/>
                                    </Border.Background>

                                </Border>

                                <TextBlock Grid.Column="1" Margin="10, 0, 0, 0" VerticalAlignment="Center" Text="{Binding BlueCurrentGameItemDetailPropertys[0].SummonerName, UpdateSourceTrigger=PropertyChanged}"/>
                            </Grid>

                        </StackPanel>

                    </StackPanel>

                    <Border 
